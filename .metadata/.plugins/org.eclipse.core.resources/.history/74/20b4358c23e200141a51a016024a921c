package foi.hr.air.asocijacije.db;

import com.activeandroid.Model;
import com.activeandroid.annotation.Column;
import com.activeandroid.annotation.Table;

@Table(name = "Rijec")
public class Word extends Model {

	@Column(name = "id_rijeci")
	public long id_rijeci;
	@Column(name = "naziv")
	public String naziv;
	@Column(name = "id_recenice")
	public long id_recenice;
	@Column(name = "recenica")
	public Sentence recenica;

	public Word() {
		super();
	}

	public Word(long id_rijeci, String naziv, long id_recenice) {
		super();
		this.id_rijeci = id_rijeci;
		this.naziv = naziv;
		this.id_recenice = id_recenice;
	}

}
