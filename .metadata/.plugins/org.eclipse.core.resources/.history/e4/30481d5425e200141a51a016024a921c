package foi.hr.air.asocijacije.db;

import com.activeandroid.Model;
import com.activeandroid.annotation.Column;
import com.activeandroid.annotation.Table;

@Table (name = "Igra")
public class Game extends Model {

	@Column (name = "id_igre")
	public long id_igre;
	@Column (name = "naziv")
	public String naziv;
	@Column (name = "rezultat")
	public int rezultat;
	
	public Game (){
		super();
	}
	
	
	public Game(long id_igre, String naziv, int rezultat) {
		super();
		this.id_igre = id_igre;
		this.naziv = naziv;
		this.rezultat = rezultat;
	}
	
	

}
